#include<stdio.h>
void printArray(int arr[],int size){
    for(int i=0;i<size;i++){
        printf("  %d  ",arr[i]);
    }
}



void reverseArray(int A[],int revarr[],int size){

int count = 0;
int n;
while( size>-1){
  
    revarr[count] =  A[size - 1];;
    count++;
    size--;
}

}
int main(){
    
    int n;
    scanf("%d",&n);
    
    int A[100];
    
    for(int i = 0;i<n;i++){
        scanf("%d",&A[i]);
    }
    
    
 
   int arrtwo[100];
    
    
   

   reverseArray(A,arrtwo,4);

   printArray(arrtwo,4);


return 0;
}
